{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 0,
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "no-plusplus": 0,
        "comma-dangle": 0,
        "linebreak-style": 0,
        "react/state-in-constructor": 0,
        "react/no-access-state-in-setstate": 0,
    }
}